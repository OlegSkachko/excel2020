{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "quotes": "off",
    "padded-blocks": "off",
    "space-before-blocks": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off"
  },

  "extends": ["eslint:recommended", "google"]
}